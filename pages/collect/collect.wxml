<view>
  <text class='title'>我的收藏</text>
  <scroll-view 
    scroll-y
    class='scroll-style'>
    <rich-text
      class='question'
      wx:for='{{items}}'
      wx:for-item="item"
      type='a'
      bindtap='tapMsg'>{{item.msg}}</rich-text>
  </scroll-view>
  <view class='bottom-btn-bar'>
    <button class='bottom-btn' bindtap='cleanAll'>一键清除我的收藏</button>
  </view>
</view>